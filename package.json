{
  "name": "junielyfe",
  "version": "0.1.0",
  "private": true,
  "description": "Career & finance copilot MVP - Turborepo monorepo",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:types": "supabase gen types typescript --linked > packages/db/src/types.ts",
    "db:migrate": "supabase db push --linked",
    "db:reset": "supabase db reset --linked",
    "db:link": "supabase link --project-ref jwpopvvaoipjjslrlpmg",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@junielyfe/eslint-config": "workspace:*",
    "@junielyfe/tsconfig": "workspace:*",
    "@types/node": "^20",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
