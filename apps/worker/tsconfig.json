{
  "extends": "@junielyfe/tsconfig/node.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": false,
    "paths": {
      "@junielyfe/core": ["../../packages/core/src"],
      "@junielyfe/db": ["../../packages/db/src"],
      "@junielyfe/agents": ["../../packages/agents/src"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}